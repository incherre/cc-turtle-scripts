--A script to bring things up

for i = 1, 16 do
  turtle.select(i)
  if turtle.getItemCount > 0 then
    while turtle.up() do end
    for j = i, 16 do
      turtle.select(j)
      turtle.drop()
    end
    break
  end
end

while true do
  while turtle.down() do end
  for i = 1, 16 do
    while not turtle.suck() do
      os.sleep(30)
    end
  end
  while turtle.up() do end
  for i = 1, 16 do
    turtle.select(i)
    turtle.drop()
  end
end
